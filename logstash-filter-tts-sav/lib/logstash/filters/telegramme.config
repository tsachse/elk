#####################################################################
# FST-Sendungs-Requests FST-Client -> FST-Server
#####################################################################

# Sendungs-Daten holen
[FST1]
   wsname	S 16 	# Workstation-Name (DNS-Name)
   idcode	S 30	# Gali-Code (TBBSSSSAAP) oder WA-Barcode oder Uml-Barcode
   kzmodus	C 1	# Sendungserfassung, Ueberwarenerfassung, NSF-AK
   fid_bereich  S 2	# Lagerorts-Bereich fuer FID
   dmode_eti	S 4	# Modus Etikettendruck (fuer Direktverarbeitung)
   dname_eti	S 80	# Name Etikettendrucker (fuer Direktverarbeitung)


####################################################################
# Sendungsabschluss-Requests FST-Client -> FST-Server
# ob Erfassung, Bearbeitung, usw. abhaengig von kzmodus 
####################################################################

[FSTBEA]
   wsname	S 16 	# Workstation-Name (DNS-Name)
   fid_bereich  S 2	# Lagerorts-Bereich fuer FID
   lfd_tag	I 3
   bat_nr	I 2
   sen_nr	I 4
   art_za	I 2
   kzmodus	C 1	# Sendungserfassung, Ueberwarenerfassung, NSF-AK
   kzanfrage	C 1	# Lageranfrage/Reduzierung/wartenUeberware/keineAktion
   kzrech	C 1	# Druck Rechnung ja/nein (in sendkompl bereitstellen)
   kzetikett	C 1     # Druck Etikett ja/nein (eigentlich immer ja!)
   kzherkunft	C 1	# Herkunftskz bei Ueberware
   nrrutsche	I 4	# Rutschennummer bei Ueberware
   anz_fehlart	I 2	# Anzahl fehlende AK 
   anz_artikel	I 2
   kzftyp	S 100	# w,e,f,s,x,m bei Sendungserfassung oder NSF-AK
   dmode_eti	S 4	# Modus Etikettendruck (dev|lp|ip)
   dname_eti	S 80	# Name Etikettendrucker (devicefile|druckername|ip-adresse)

# zu anz_fehlart:
# bei Sortierfehlern manuelle eingabe (keine sonstigen artikelmarkierungen), 
# sonst markierte artikel zaehlen 
# zu kzftyp:
# bei NSF: konstant s-SPP


####################################################################
# Verwaltungsfunktionen-Requests Web-Client -> FST-Server
####################################################################

# Druck von Rechnungen aus dem Druckpool oder fuer eine Richtungsgruppe
# kombinationen: kztermin normal (0) oder eil (1) oder alles (2) 
#		 fuer eine richtungsgruppe oder alle (0)
[WEBREDR]
   wsname	S 16 	# Workstation-Name (DNS-Name)
   kztermin	C 1	# Normal- oder Eilsendungen
   gruppe       I 2	# Richtungsgruppe
   kennung      C 1	# Kennbuchstabe Lagerortsbereich

# NEU, 27.10.09 
# bei K und T auch teilbereiche beachten
[WEBDR]
   wsname	S 16 	# Workstation-Name (DNS-Name)
   kztermin	C 1	# Normal- oder Eilsendungen
   gruppe       I 2	# Richtungsgruppe
   kennung      C 1	# Kennbuchstabe Lagerortsbereich
   von_ber	S 4	# Bereich von
   bis_ber	S 4	# Bereich bis

# Raeumen von FST-Sendungen fuer eine FID-Bereich 
# bzw. eine Richtungsgruppe (<RG>A0000 - <RG>Z9999)
[WEBRAUM]
   wsname	S 16 	# Workstation-Name (DNS-Name)
   lfd_tag	I 3	# Filter fuer einen LV-Tag
   von_fid	S 7	# Bereich der zu raeumenden FST-Sendungen
   bis_fid	S 7	# 

# Druckanstoss fuer registrierte ARL-Soko-Fehler
[WEBAFS]
   lfd_tag	I 3	# Filter fuer einen LV-Tag
   anz_sen	I 4	# maximale Anzahl Sendungen

# Fehlerbehandlung fuer offene (nicht gedruckte) ARL-Soko
# mit Lageranfrage oder sofort Druckanstoss
# WEB-(A)rl-(S)oko-(O)ffen
[WEBASO]
   lfd_tag      I 3     # Filter fuer einen LV-Tag
   kzanfrage    C 1     # Lageranfrage oder nicht (dann nur Rechnungsdruck)

